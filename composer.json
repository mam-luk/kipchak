{
  "name": "mamluk/kipchak",
  "type": "library",
  "description": "An API Development Kit (ADK) to consistently build and manage APIs",
  "keywords": [
    "APIs",
    "microservices"
  ],
  "homepage": "https://mamluk.net",
  "license": "MIT",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=8.2",
    "bryanjhv/slim-session": "^4.1",
    "doctrine/orm": "^2.17",
    "firebase/php-jwt": "^6.9",
    "guzzlehttp/guzzle": "^7.8",
    "illuminate/http": "^9.52",
    "monolog/monolog": "^3.5",
    "php-di/php-di": "^6.4",
    "slim/psr7": "^1.6",
    "slim/slim": "^4.12",
    "symfony/cache": "^6.3",
    "zircote/swagger-php": "^4.7",
    "mamluk/couchdb": "0.6",
    "cuyz/valinor": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Mamluk\\Kipchak\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mamluk\\Kipchak\\Tests\\": "tests/"
    }
  }
}
