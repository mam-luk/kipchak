{
  "name": "mamluk/kipchak",
  "type": "library",
  "description": "A PHP Library to consistently build microservices and APIs",
  "keywords": [
    "APIs",
    "microservices"
  ],
  "homepage": "https://mamluk.net",
  "license": "MIT",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=8.1",
    "bryanjhv/slim-session": "~4.0",
    "doctrine/orm": "^2.13",
    "firebase/php-jwt": "^6.3",
    "guzzlehttp/guzzle": "^7.4",
    "illuminate/http": "^9.23",
    "monolog/monolog": "^3.2",
    "php-di/php-di": "^6.4",
    "slim/psr7": "^1.5",
    "slim/slim": "4.*",
    "symfony/cache": "^6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Mamluk\\Kipchak\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mamluk\\Kipchak\\Tests\\": "tests/"
    }
  }
}
